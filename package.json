{
  "name": "monod",
  "private": true,
  "version": "1.0.0",
  "description": "monorepo-nodejs-stack",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "preview": "pnpm run preview:web; pnpm run preview:admin",
    "preview:web": "pnpm --filter web run preview",
    "preview:admin": "pnpm --filter admin run preview",
    "lint": "turbo lint",
    "clean": "turbo clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "keywords": [],
  "author": "Mahabub Alam <mahabubx7@gmail.com>",
  "license": "UNLICENSED",
  "packageManager": "pnpm@8.6.0",
  "engines": {
    "node": ">=18.16.x"
  },
  "devDependencies": {
    "eslint": "^8.41.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "^2.8.8",
    "turbo": "^1.10.1",
    "typescript": "^5.1.3"
  }
}
